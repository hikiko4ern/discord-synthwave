{
  "name": "discord-synthwave",
  "version": "1.3.0",
  "description": "SynthWave '84 theme for Discord code blocks",
  "type": "module",
  "scripts": {
    "postinstall": "husky install",
    "version": "yarn build",
    "clean": "rimraf dist",
    "build": "npm-run-all -s clean -p 'build:*'",
    "build:css": "yarn node scripts/build/css.js",
    "build:loaders": "yarn node scripts/build/loaders.js"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.45",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": "^17.5.0 || ^18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hikiko4ern/discord-synthwave.git"
  },
  "author": "hikiko4ern",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.2.3",
  "private": true
}
